{
  "name": "cbl-ionic",
  "version": "0.1.7",
  "description": "Ionic Capacitor plugin for Couchbase Lite Enterprise (3.x+)",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CblIonic.podspec"
  ],
  "author": "Developer Experience/Ecosystem Team @ Couchbase",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Couchbase-Ecosystem/cbl-ionic.git"
  },
  "bugs": {
    "url": "https://github.com/Couchbase-Ecosystem/cbl-ionic/issues"
  },
  "keywords": [
    "couchbase",
    "couchbase-lite",
    "ionic",
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run build && npm run verify:ios && npm run verify:android",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api IonicCouchbaseLitePlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^5.7.5",
    "@capacitor/core": "^5.7.5",
    "@capacitor/ios": "^5.7.5",
    "@capacitor/docgen": "^0.0.18",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.35.0",
    "prettier": "~2.3.0",
    "prettier-plugin-java": "~1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "swiftlint": "^1.0.1",
    "tslib": "^2.6.2",
    "typescript": "5.4.2",
    "chai": "^5.1.0"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.7.5"
  },
  "swiftlint": "@ionic/swiftlint-config",
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6"
  }
}
